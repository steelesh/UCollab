{
  "name": "ucollab",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "services:up": "docker compose up -d",
    "services:down": "docker compose down",
    "services:logs": "docker compose logs -f",
    "dev": "npm run dev:local",
    "predev:local": "tsx scripts/check-dev-setup.ts",
    "dev:local": "dotenv -e .env -- next dev & npx prisma studio --browser none",
    "dev:dev": "dotenv -e .env.development -- next dev & npx prisma studio --browser none",
    "dev:test": "dotenv -e .env.test -- next dev & npx prisma studio --browser none",
    "dev:prod": "dotenv -e .env.production -- next dev & npx prisma studio --browser none",
    "build": "next build",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "start": "next start",
    "test:e2e": "playwright test --config playwright.config.ts",
    "db:generate": "prisma migrate dev",
    "db:migrate": "prisma migrate deploy",
    "db:push": "prisma db push",
    "db:studio": "prisma studio",
    "db:clean": "prisma db push --force-reset",
    "postinstall": "prisma generate"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^2.7.4",
    "@aws-sdk/client-s3": "^3.750.0",
    "@dicebear/collection": "^9.2.2",
    "@dicebear/core": "^9.2.2",
    "@prisma/client": "^6.4.1",
    "@t3-oss/env-nextjs": "^0.12.0",
    "@tailwindcss/postcss": "^4.0.8",
    "bullmq": "^5.41.5",
    "dotenv-cli": "^8.0.0",
    "http-status-codes": "^2.3.0",
    "motion": "^12.4.7",
    "next": "^15.1.7",
    "next-auth": "^5.0.0-beta.25",
    "next-themes": "^0.4.4",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@playwright/test": "^1.50.1",
    "@types/eslint": "^9.6.1",
    "@types/eslint-plugin-jsx-a11y": "^6.10.0",
    "@types/node": "^22.13.5",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@typescript-eslint/eslint-plugin": "^8.24.1",
    "@typescript-eslint/parser": "^8.24.1",
    "daisyui": "^5.0.0-beta.6",
    "eslint": "^9.21.0",
    "eslint-config-next": "^15.1.7",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-playwright": "^2.2.0",
    "eslint-plugin-prettier": "^5.2.3",
    "postcss": "^8.5.3",
    "prettier": "3.4.2",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "prisma": "^6.4.1",
    "tailwindcss": "^4.0.8",
    "tsx": "^4.19.3",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.24.1"
  },
  "ct3aMetadata": {
    "initVersion": "7.37.0"
  },
  "packageManager": "npm@11.1.0"
}
