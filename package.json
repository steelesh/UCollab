{
  "name": "ucollab",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "services:up": "docker compose up -d",
    "services:down": "docker compose down",
    "services:logs": "docker compose logs -f",
    "dev": "npm run dev:local",
    "predev:local": "tsx scripts/check-dev-setup.ts",
    "dev:local": "dotenv -e .env -- next dev & npx prisma studio --browser none",
    "dev:dev": "dotenv -e .env.development -- next dev & npx prisma studio --browser none",
    "dev:test": "dotenv -e .env.test -- next dev & npx prisma studio --browser none",
    "dev:prod": "dotenv -e .env.production -- next dev & npx prisma studio --browser none",
    "build": "next build",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "start": "next start",
    "test:unit": "vitest run --config src/tests/config/vitest.unit.config.ts",
    "test:integration": "vitest run --config src/tests/config/vitest.integration.config.ts",
    "test:e2e": "playwright test --config src/tests/config/playwright.config.ts",
    "test:e2e:smoke": "playwright test --config src/tests/config/playwright.config.ts --project=smoke",
    "test:all": "npm run test:unit && npm run test:integration && npm run test:e2e",
    "db:generate": "prisma migrate dev",
    "db:migrate": "prisma migrate deploy",
    "db:push": "prisma db push",
    "db:studio": "prisma studio",
    "db:init": "prisma db push && prisma db seed",
    "db:clean": "prisma db push --force-reset",
    "postinstall": "prisma generate",
    "db:seed": "prisma db seed",
    "prepare": "husky"
  },
  "prisma": {
    "seed": "tsx src/data/seed.ts"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^2.7.4",
    "@aws-sdk/client-s3": "^3.714.0",
    "@dicebear/collection": "^9.2.2",
    "@dicebear/core": "^9.2.2",
    "@eslint/eslintrc": "^3.2.0",
    "@prisma/client": "^6.3.0",
    "@t3-oss/env-nextjs": "^0.12.0",
    "@tailwindcss/postcss": "^4.0.2",
    "bullmq": "^5.34.10",
    "dotenv-cli": "^8.0.0",
    "eslint-config-next": "^15.1.6",
    "http-status-codes": "^2.3.0",
    "motion": "^12.0.6",
    "next": "^15.1.6",
    "next-auth": "^5.0.0-beta.25",
    "next-themes": "^0.4.4",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "swr": "^2.3.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@playwright/test": "^1.49.1",
    "@types/eslint": "^9.6.1",
    "@types/node": "^22.10.7",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.3",
    "@typescript-eslint/eslint-plugin": "^8.16.0",
    "@typescript-eslint/parser": "^8.16.0",
    "daisyui": "^5.0.0-beta.6",
    "eslint": "^9.1.9",
    "eslint-config-next": "^15.1.6",
    "eslint-config-prettier": "^10.0.1",
    "husky": "^9.1.7",
    "postcss": "^8.5.1",
    "prettier": "3.4.2",
    "prettier-plugin-tailwindcss": "^0.6.9",
    "prisma": "^6.3.0",
    "prisma-generator-fake-data": "^0.14.3",
    "tailwindcss": "^4.0.2",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2"
  },
  "overrides": {
    "cookie": "^0.7.2"
  },
  "ct3aMetadata": {
    "initVersion": "7.37.0"
  },
  "packageManager": "npm@10.9.0"
}
